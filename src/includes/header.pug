header.header
	.container.header__container
		.header__logo
			a(href="#").logo ЯС
		.header__menu
			nav.nav
			- var menu = ["Главная", "Обо мне", "Навыки","Работы", "Отзывы", "Связаться"]
				ul.nav__list
					each item in menu
						li.nav__item
							a(href="#").nav__link #{item}
		.header__links
		- var icons = ["insta", "vk", "github"]
			ul.socials
			each item in icons
				li.socials__item
					a(href="#").socials__link
						+icon(`${item}`, "socials__icon")